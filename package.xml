<?xml version="1.0" encoding="UTF-8"?>
<package name="de.softcreatr.show.extendedSearch" xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com https://www.woltlab.com/XSD/2019/package.xsd">
